<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="itd.secondhandmall.dao.RecordsMapper">

    <sql id="Base_Column_List" >
        id,tid,stime,price,attributes
    </sql>


    <select id="selectAllByUserId" resultType="itd.secondhandmall.pojo.Records">
        select
        <include refid="Base_Column_List"/>
        from
        records
        where
        id = #{id}
    </select>

    <insert id="insertRecord" parameterType="itd.secondhandmall.pojo.Records">
        insert into records(id,tid,stime,price,attributes)
        values(#{id},#{tid},#{stime},#{price},#{attributes})
    </insert>
</mapper>